<div>
  <a href="/">Back home</a>
</div>
<div class="sliding animated fadeInLeft slower">
<h3>Comedy</h3>
</div>
<div class="collection">
  {{#each comedy}}
  <table class="{{genre}} animated bounceInLeft slow delay-2s">
    <tr>
      <td colspan="2">
        <div class="thumbnail">
          <img src="{{thumbnail}}" alt=""> 
        </div>
      </td>
    </tr>
    <tr>
      <td><strong>Name: </strong></td>
      <td>{{name}}</td>
    </tr>
    <tr>
      <td><strong>Artist: </strong></td>
      <td>{{artist}}</td>
    </tr>
  </table>
  {{/each}}
</div>


